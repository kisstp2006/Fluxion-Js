<Scene name="AllNodesScene">
    <Camera name="MainCamera" x="0" y="0" zoom="1" rotation="0" />
    
    <!-- Standard Sprite -->
    <Sprite name="LogoSprite" imageSrc="../../Fluxion/Icon/Fluxion_icon.png" x="-0.5" y="0" width="0.5" height="0.5" />
    
    <!-- Animated Sprite 
         Using the same image as a "spritesheet" just to demonstrate the node.
         Assuming the image is the frame itself. 
    -->
    <AnimatedSprite name="AnimSprite" imageSrc="../../Fluxion/Icon/Fluxion_icon.png" x="0.5" y="0" width="0.5" height="0.5" frameWidth="512" frameHeight="512">
        <Animation name="spin" frames="0" speed="0.5" loop="true" autoplay="true" />
    </AnimatedSprite>
    
    <!-- Audio Node -->
    <Audio name="BackgroundAudio" src="../../Examples/Jolly3Chapter2Elevator/assets/sounds/elevator loop.mp3" loop="true" volume="0.2" autoplay="true" />
</Scene>